  <% if (user && user.analyticsEnabled && analyticsData) { %>
  <script<%- typeof cspNonce !== 'undefined' ? ` nonce="${cspNonce}"` : '' %>>
    // Chart.js integration (uses shared chart-utils.js for chart management)
    const API_BASE = '/api/analytics';
    let currentRange = 30;
    let currentLocation = null;

    // Initialize on page load
    document.addEventListener('DOMContentLoaded', () => {
      initializeFilters();
      loadTrendCharts();
    });

    // Filter Event Listeners
    function initializeFilters() {
      document.querySelectorAll('.filter-btn').forEach(btn => {
        btn.addEventListener('click', (e) => {
          document.querySelectorAll('.filter-btn').forEach(b => b.classList.remove('active'));
          e.target.classList.add('active');
          currentRange = parseInt(e.target.dataset.range);
          loadAnalyticsData();
        });
      });

      const locationFilter = document.getElementById('location-filter');
      if (locationFilter) {
        locationFilter.addEventListener('change', (e) => {
          currentLocation = e.target.value || null;
          loadAnalyticsData();
        });
      }
    }

    // Load trend charts
    async function loadTrendCharts() {
      try {
        const params = new URLSearchParams({
          startDate: new Date(Date.now() - currentRange * 24 * 60 * 60 * 1000).toISOString().split('T')[0],
          endDate: new Date().toISOString().split('T')[0]
        });

        if (currentLocation) params.append('location', currentLocation);

        const response = await fetch(`${API_BASE}/trends?${params}`);
        const trends = await response.json();

        updateSparkline('rating-chart', trends.averageRating || []);
        updateSparkline('reviews-chart', trends.reviewsPositive || []);
        updateSparkline('click-chart', trends.clickRate || []);
        updateSparkline('conversion-chart', trends.conversionRate || []);
      } catch (error) {
        console.error('Failed to load trends:', error);
      }
    }

    // Load analytics data on filter change
    async function loadAnalyticsData() {
      try {
        const endDate = new Date();
        const startDate = new Date();
        startDate.setDate(startDate.getDate() - currentRange);

        const params = new URLSearchParams({
          startDate: startDate.toISOString().split('T')[0],
          endDate: endDate.toISOString().split('T')[0]
        });

        if (currentLocation) params.append('location', currentLocation);

        const response = await fetch(`${API_BASE}/metrics?${params}`);
        const metrics = await response.json();

        // Update display
        document.getElementById('roi-display').textContent = metrics.roi.roiFormatted;
        document.getElementById('avg-rating').textContent = metrics.reviews.averageRating.toFixed(1);
        document.getElementById('total-reviews').textContent = metrics.reviews.total;
        document.getElementById('click-rate').textContent = metrics.requests.clickRate.toFixed(1) + '%';
        document.getElementById('conversion-rate').textContent = metrics.requests.conversionRate.toFixed(1) + '%';

        // Reload charts
        loadTrendCharts();
      } catch (error) {
        console.error('Failed to load analytics:', error);
      }
    }

    // Update Sparkline Chart (uses shared chart-utils.js)
    function updateSparkline(chartId, data) {
      createSparkline(chartId, data, {
        backgroundColor: 'rgba(255, 186, 73, 0.15)',
        pointHoverBackgroundColor: '#500B42'
      });
    }
  </script>
  <% } %>

  <script<%- typeof cspNonce !== 'undefined' ? ` nonce="${cspNonce}"` : '' %>>
    // Toggle collapsible sections
    function toggleSection(sectionId) {
      var section = document.getElementById(sectionId);
      if (!section) {
        console.error('Section not found:', sectionId);
        return;
      }

      var body = section.querySelector('.section-body');
      var arrow = section.querySelector('.section-arrow');

      if (body && arrow) {
        body.classList.toggle('collapsed');
        arrow.classList.toggle('rotated');
      }
    }

    // Initialize collapsible sections
    document.addEventListener('DOMContentLoaded', function() {
      // Add click handlers to all section headers
      var sectionHeaders = document.querySelectorAll('.section-header');
      sectionHeaders.forEach(function(header) {
        header.style.cursor = 'pointer';
        header.addEventListener('click', function(e) {
          var section = this.closest('.section.collapsible');
          if (section) {
            var sectionId = section.id;
            toggleSection(sectionId);
          }
        });
      });
    });

    // Send SMS Form Handler
    document.addEventListener('DOMContentLoaded', function() {
      // Attach form submit handler (CSP-compliant, no inline handlers)
      const quickTestForm = document.getElementById('quick-test-form');
      console.log('DEBUG: quickTestForm found:', !!quickTestForm);
      if (quickTestForm) {
        // Use capture phase to intercept before any other handlers
        quickTestForm.addEventListener('submit', function(event) {
          console.log('DEBUG: Form submit intercepted (capture phase)');
          event.preventDefault();
          event.stopImmediatePropagation();
          sendTestSms(event);
          return false;
        }, true); // true = capture phase

        // Also add to submit button directly
        const submitBtn = quickTestForm.querySelector('button[type="submit"]');
        if (submitBtn) {
          submitBtn.addEventListener('click', function(event) {
            console.log('DEBUG: Submit button clicked');
            event.preventDefault();
            event.stopImmediatePropagation();
            // Manually trigger sendTestSms
            const fakeEvent = { target: quickTestForm, preventDefault: () => {} };
            sendTestSms(fakeEvent);
            return false;
          }, true);
        }
        console.log('DEBUG: Submit handlers attached');
      }

      // Attach phone input formatter (CSP-compliant, no inline handlers)
      const testPhoneInput = document.getElementById('test-phone');
      if (testPhoneInput) {
        testPhoneInput.addEventListener('input', function() {
          formatPhoneInput(this);
        });
      }

      // Onboarding modal event handlers (CSP-compliant)
      const onboardingTestSms = document.getElementById('onboarding-test-sms');
      if (onboardingTestSms) {
        onboardingTestSms.addEventListener('click', function(e) {
          e.preventDefault();
          closeWelcomeModal();
          document.getElementById('quickTestHeader')?.click();
          document.getElementById('test-phone')?.focus();
        });
      }

      const welcomeDismissBtn = document.getElementById('welcome-dismiss-btn');
      if (welcomeDismissBtn) {
        welcomeDismissBtn.addEventListener('click', function() {
          closeWelcomeModal();
        });
      }
    });

    // Phone number formatting
    function formatPhoneInput(input) {
      let value = input.value.replace(/\D/g, ''); // Remove non-digits
      let formatted = '';

      if (value.length > 0) {
        formatted = '(' + value.substring(0, 3);
      }
      if (value.length >= 4) {
        formatted += ') ' + value.substring(3, 6);
      }
      if (value.length >= 7) {
        formatted += '-' + value.substring(6, 10);
      }

      input.value = formatted;
    }

    // Convert formatted phone to E.164
    function toE164(formatted) {
      const digits = formatted.replace(/\D/g, '');
      if (digits.length === 10) {
        return '+1' + digits;
      }
      return null;
    }

    // Send test SMS
    async function sendTestSms(event) {
      event.preventDefault();

      const form = event.target;
      const phoneInput = document.getElementById('test-phone');
      const nameInput = document.getElementById('test-name');
      const resultDiv = document.getElementById('test-result');
      const submitBtn = document.getElementById('send-test-btn');

      // Get values
      const formattedPhone = phoneInput.value.trim();
      const e164Phone = toE164(formattedPhone);
      const customerName = nameInput.value.trim();

      // Validate
      if (!e164Phone) {
        showResult('error', 'Please enter a valid 10-digit US phone number');
        return;
      }

      // Confirmation
      const displayName = customerName || 'Test Customer';
      if (!confirm(`Send test SMS to ${formattedPhone}?\n\nCustomer: ${displayName}`)) {
        return;
      }

      // Disable button
      submitBtn.disabled = true;
      submitBtn.textContent = 'Sending...';

      try {
        const csrfToken = document.getElementById('csrf-token')?.value || '';
        console.log('DEBUG: CSRF token from form:', csrfToken ? csrfToken.substring(0,8) + '...' : 'EMPTY');

        // Include _csrf in body for more reliable CSRF validation
        const response = await fetch('/dashboard/send-test-sms', {
          method: 'POST',
          credentials: 'same-origin', // Ensure cookies are sent
          headers: {
            'Content-Type': 'application/json',
            'Accept': 'application/json',
            'X-CSRF-Token': csrfToken
          },
          body: JSON.stringify({
            phone: e164Phone,
            customerName: customerName,
            _csrf: csrfToken  // Also include in body as backup
          })
        });

        const data = await response.json();

        if (data.success) {
          // Show toast notification
          if (typeof Toast !== 'undefined') {
            Toast.success('Test SMS sent! Check your phone');
          }

          // Show inline result with copy link
          const resultDiv = document.getElementById('test-result');
          resultDiv.className = 'test-result success';
          resultDiv.style.display = 'block';

          // Clear previous content and build new content safely
          resultDiv.textContent = '';

          var checkmark = document.createElement('span');
          checkmark.textContent = '‚úÖ ' + data.message;
          resultDiv.appendChild(checkmark);

          var linkContainer = document.createElement('div');
          linkContainer.className = 'review-link-display';

          var linkLabel = document.createElement('strong');
          linkLabel.textContent = 'Review Link:';
          linkContainer.appendChild(linkLabel);
          linkContainer.appendChild(document.createElement('br'));

          var linkAnchor = document.createElement('a');
          linkAnchor.href = data.reviewLink;
          linkAnchor.target = '_blank';
          linkAnchor.textContent = data.reviewLink;
          linkContainer.appendChild(linkAnchor);

          resultDiv.appendChild(linkContainer);

          var copyBtn = document.createElement('button');
          copyBtn.className = 'copy-link-btn';
          copyBtn.textContent = 'üìã Copy Link';
          copyBtn.addEventListener('click', function() {
            copyLink(data.reviewLink);
          });
          resultDiv.appendChild(copyBtn);

          // Clear form
          form.reset();

          // No page reload - user stays in context
        } else {
          // Show error toast
          if (typeof Toast !== 'undefined') {
            Toast.error(data.error || 'Failed to send SMS');
          }
          showResult('error', '‚ùå ' + data.error);
        }
      } catch (error) {
        console.error('Send test SMS error:', error);
        if (typeof Toast !== 'undefined') {
          Toast.error('Failed to send SMS. Please try again.');
        }
        showResult('error', '‚ùå Failed to send SMS. Please try again.');
      } finally {
        // Re-enable button
        submitBtn.disabled = false;
        submitBtn.textContent = 'Send SMS';
      }
    }

    // Show result message
    function showResult(type, message) {
      const resultDiv = document.getElementById('test-result');
      resultDiv.className = 'test-result ' + type;
      resultDiv.innerHTML = message;
      resultDiv.style.display = 'block';

      // Auto-hide after 10 seconds for errors, keep success visible
      if (type === 'error') {
        setTimeout(() => {
          resultDiv.style.display = 'none';
        }, 10000);
      }
    }

    // Copy link to clipboard
    function copyLink(link) {
      navigator.clipboard.writeText(link).then(() => {
        alert('‚úÖ Link copied to clipboard!');
      }).catch(err => {
        console.error('Failed to copy:', err);
        alert('‚ùå Failed to copy link');
      });
    }

    // Review Modal Functions
    document.addEventListener('DOMContentLoaded', function() {
      // Add click handlers to all View buttons
      document.querySelectorAll('.btn-view').forEach(function(btn) {
        btn.addEventListener('click', function() {
          const reviewDataStr = decodeURIComponent(this.dataset.review);
          const content = decodeURIComponent(this.dataset.content || '');
          showReviewModal(reviewDataStr, content);
        });
      });

      // Add click handler for modal close button
      const closeBtn = document.getElementById('modalCloseBtn');
      if (closeBtn) {
        closeBtn.addEventListener('click', function(e) {
          e.preventDefault();
          e.stopPropagation();
          closeReviewModal();
        });
      }

      // Close modal when clicking outside
      const modal = document.getElementById('reviewModal');
      if (modal) {
        modal.addEventListener('click', function(e) {
          if (e.target === modal) {
            closeReviewModal();
          }
        });
      }
    });

    function showReviewModal(reviewDataStr, content) {
      try {
        const reviewData = JSON.parse(reviewDataStr);
        const modal = document.getElementById('reviewModal');

        // Populate modal with review data
        document.getElementById('modal-customer-name').textContent = reviewData.customerName || 'Anonymous';
        document.getElementById('modal-rating').innerHTML = '‚≠êÔ∏è'.repeat(reviewData.rating) + ' (' + reviewData.rating + '/5)';

        // Format date
        const date = new Date(reviewData.createdAt);
        document.getElementById('modal-date').textContent = date.toLocaleDateString('en-US', {
          year: 'numeric',
          month: 'long',
          day: 'numeric',
          hour: '2-digit',
          minute: '2-digit'
        });

        // Set content
        document.getElementById('modal-content').textContent = content || 'No feedback provided';

        // Show modal
        modal.style.display = 'flex';
      } catch (error) {
        console.error('Error showing review modal:', error);
        alert('Failed to load review details');
      }
    }

    function closeReviewModal() {
      document.getElementById('reviewModal').style.display = 'none';
    }

    // Welcome Modal Functions
    function showWelcomeModal() {
      const modal = document.getElementById('welcomeModal');
      if (modal) {
        modal.style.display = 'flex';
      }
    }

    function closeWelcomeModal() {
      const modal = document.getElementById('welcomeModal');
      if (modal) {
        modal.style.display = 'none';
        // Remember that user dismissed the modal
        localStorage.setItem('morestars_welcome_dismissed', 'true');
      }
    }

    // Show welcome modal on first visit
    document.addEventListener('DOMContentLoaded', function() {
      const welcomeModal = document.getElementById('welcomeModal');
      if (welcomeModal && !localStorage.getItem('morestars_welcome_dismissed')) {
        // Small delay to let page render first
        setTimeout(function() {
          showWelcomeModal();
        }, 500);
      }

      // Close welcome modal on outside click
      if (welcomeModal) {
        welcomeModal.addEventListener('click', function(e) {
          if (e.target === welcomeModal) {
            closeWelcomeModal();
          }
        });
      }

      // Close welcome modal on Escape key
      document.addEventListener('keydown', function(e) {
        if (e.key === 'Escape' && welcomeModal && welcomeModal.style.display === 'flex') {
          closeWelcomeModal();
        }
      });
    });
  </script>

